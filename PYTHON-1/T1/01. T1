# In[ ]:
x=int(input("Enter number:"))
z=x%10
if(z==0):
    print("zero")
elif (z==1):
    print("one")
elif (z==2):
    print("two")
elif (z==3):
    print("three")
else:
    print("number is not =0,1,2,3")
