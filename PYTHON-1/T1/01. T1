# In[ ]:
x1=int(input("Enter number:"))
sum=0
temp=x1
last=x1%10
first=0
middle=0
c=0
while x1>0:
    c=c+1
    digit=x1%10
    first=digit
    x1=x1//10
    middle=(temp%10**(c-1))//10
    swap=((last*10**(c-1))+(middle*10)+first)
print(f"first=",first)
print(f"last=",last)
print("middle=",middle)
print("swap of first and last number="swap)
-------------------------------------------------------------------------------------------
# In[ ]:
x1=int(input("Enter number:"))
temp=x1
c=0
while x1>0:
    c=c+1
    x1=x1//10
    second=(temp//10**(c-2))
print("first 2 number is",second)
-------------------------------------------------------------------------------------------
# In[12]:
x1=int(input("Enter number:"))
sum=0
temp=x1
c=0
while x1>0:
    x1=x1//10
    c=c+1
while temp>0:
    digit=temp%10
    sum=sum+(digit**c)
    temp=temp//10
print(f"sum of cube of number in base of count=",sum)
-------------------------------------------------------------------------------------------
