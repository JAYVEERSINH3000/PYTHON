# In[ ]:
x1=int(input("Enter number:"))
sum=0
while x1>0:
    digit=x1%10
    sum=sum+(digit**3)
    x1=x1//10
print(f"sum of cube of number=",sum)
-------------------------------------------------------------------------------------------
# In[ ]:
x1=int(input("Enter number:"))
sum=0
temp=x1
while x1>0:
    digit=x1%10
    sum=sum+(digit**3)
    x1=x1//10
    
print(sum)
if(sum==temp):
    print(f"yes it is an armstrong number")
else:
    print("no it is not")
